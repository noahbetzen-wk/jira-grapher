{
    "name": "Jira Grapher",
    "version": "1.0",
    "description": "Creates an interactive graph of Jira dependencies",
    "manifest_version": 2,
    "browser_action": {
        "default_icon": "icon.png"
    },
    "background": {
        "scripts": ["background.js"],
        "persistent": false
    },
    "permissions": ["activeTab"]
  }
  